

local player = Solara.Players.LocalPlayer -- Adjusted to Solara's player system
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local camera = workspace.CurrentCamera

-- Initial Key: raxzyONtop
local initialKey = "raxzyONtop"

-- Private/Paid Key: 80BADF611E8BE210EBD5E767B
local privateKey = "80BADF611E8BE210EBD5E767B"

-- Create GUI for Solara (assuming Solara has its own GUI system)
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "LockOnGui"
ScreenGui.Parent = player.PlayerGui -- Make sure this is compatible with Solara's PlayerGui system
ScreenGui.BackgroundColor3 = Color3.fromRGB(255, 182, 193) -- Pink background for the entire GUI

-- Add a "Raxzy" label at the top of the GUI
local TitleLabel = Instance.new("TextLabel")
TitleLabel.Size = UDim2.new(0, 200, 0, 50)
TitleLabel.Position = UDim2.new(0.5, -100, 0, 10)
TitleLabel.Text = "Raxzy"
TitleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TitleLabel.TextSize = 24
TitleLabel.BackgroundTransparency = 1
TitleLabel.Parent = ScreenGui

