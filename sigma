-- Free Tab: ESP (Extra Sensory Perception)
local ESPLabel = Instance.new("TextLabel")
ESPLabel.Size = UDim2.new(0, 200, 0, 50)
ESPLabel.Position = UDim2.new(0.5, -100, 0, 70)
ESPLabel.Text = "ESP (Free Feature)"
ESPLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
ESPLabel.TextSize = 18
ESPLabel.BackgroundTransparency = 1
ESPLabel.Parent = FreePage

-- Placeholder ESP functionality (can be customized further for real ESP effects)
local function createESP(player)
    local espPart = Instance.new("Part")
    espPart.Size = Vector3.new(1, 1, 1)
    espPart.Shape = Enum.PartType.Ball
    espPart.Color = Color3.fromRGB(255, 0, 0) -- Red ESP
    espPart.Anchored = true
    espPart.CanCollide = false
    espPart.Parent = workspace
    espPart.Position = player.Character.PrimaryPart.Position + Vector3.new(0, 5, 0)

    -- Simple loop to update ESP position (we can improve this for real ESP effects)
    game:GetService("RunService").Heartbeat:Connect(function()
        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            espPart.Position = player.Character.HumanoidRootPart.Position + Vector3.new(0, 5, 0)
        end
    end)
end

-- Button to toggle ESP on the Free Page
local ESPButton = Instance.new("TextButton")
ESPButton.Size = UDim2.new(0, 200, 0, 50)
ESPButton.Position = UDim2.new(0.5, -100, 0, 150)
ESPButton.Text = "Toggle ESP"
ESPButton.TextColor3 = Color3.fromRGB(255, 255, 255)
ESPButton.BackgroundColor3 = Color3.fromRGB(255, 105, 180) -- Pink button
ESPButton.Parent = FreePage

ESPButton.MouseButton1Click:Connect(function()
    for _, targetPlayer in pairs(Solara.Players:GetPlayers()) do
        if targetPlayer ~= player then
            createESP(targetPlayer)
        end
    end
end)

-- Initial Key System: raxzyONtop
local keyInputFrame = Instance.new("Frame")
keyInputFrame.Size = UDim2.new(0, 300, 0, 200)
keyInputFrame.Position = UDim2.new(0.5, -150, 0.5, -100)
keyInputFrame.BackgroundColor3 = Color3.fromRGB(255, 182, 193)
keyInputFrame.Parent = ScreenGui

local keyLabel = Instance.new("TextLabel")
keyLabel.Size = UDim2.new(0, 200, 0, 50)
keyLabel.Position = UDim2.new(0.5, -100, 0, 10)
keyLabel.Text = "Enter Key: raxzyONtop"
keyLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
keyLabel.TextSize = 18
keyLabel.BackgroundTransparency = 1
keyLabel.Parent = keyInputFrame

local keyTextBox = Instance.new("TextBox")
keyTextBox.Size = UDim2.new(0, 200, 0, 50)
keyTextBox.Position = UDim2.new(0.5, -100, 0, 70)
keyTextBox.PlaceholderText = "raxzyONtop"
keyTextBox.ClearTextOnFocus = true
keyTextBox.Parent = keyInputFrame

local submitButton = Instance.new("TextButton")
submitButton.Size = UDim2.new(0, 200, 0, 50)
submitButton.Position = UDim2.new(0.5, -100, 0, 130)
submitButton.Text = "Submit"
submitButton.TextColor3 = Color3.fromRGB(255, 255, 255)
submitButton.BackgroundColor3 = Color3.fromRGB(255, 105, 180) -- Pink button
submitButton.Parent = keyInputFrame

-- Key Validation
submitButton.MouseButton1Click:Connect(function()
    if keyTextBox.Text == initialKey then
        -- Key is correct, show Free Tab
        keyInputFrame.Visible = false
        FreePage.Visible = true
    else
        keyLabel.Text = "Invalid Key! Try Again."
        keyLabel.TextColor3 = Color3.fromRGB(255, 0, 0) -- Red text for wrong key
    end
end)

-- Private/Paid Tab: Key system
local PaidKeyTextBox = Instance.new("TextBox")
PaidKeyTextBox.Size = UDim2.new(0, 200, 0, 50)
PaidKeyTextBox.Position = UDim2.new(0.5, -100, 0, 70)
PaidKeyTextBox.PlaceholderText = "Enter Paid Key"
PaidKeyTextBox.ClearTextOnFocus = true
PaidKeyTextBox.Parent = PaidPage

local PaidKeyStatusLabel = Instance.new("TextLabel")
PaidKeyStatusLabel.Size = UDim2.new(0, 200, 0, 50)
PaidKeyStatusLabel.Position = UDim2.new(0.5, -100, 0, 130)
PaidKeyStatusLabel.Text = ""
PaidKeyStatusLabel.TextColor3 = Color3.fromRGB(255, 0, 0) -- Red text for incorrect key
PaidKeyStatusLabel.TextSize = 18
PaidKeyStatusLabel.BackgroundTransparency = 1
PaidKeyStatusLabel.Parent = PaidPage

local unlockPaidButton = Instance.new("TextButton")
unlockPaidButton.Size = UDim2.new(0, 200, 0, 50)
unlockPaidButton.Position = UDim2.new(0.5, -100, 0, 190)
unlockPaidButton.Text = "Unlock Paid Features"
unlockPaidButton.TextColor3 = Color3.fromRGB(255, 255, 255)
unlockPaidButton.BackgroundColor3 = Color3.fromRGB(255, 105, 180) -- Pink button
unlockPaidButton.Parent = PaidPage

unlockPaidButton.MouseButton1Click:Connect(function()
    if PaidKeyTextBox.Text == privateKey then
        PaidKeyStatusLabel.Text = "Features Unlocked!"
        PaidKeyStatusLabel.TextColor3 = Color3.fromRGB(0, 255, 0) -- Green text for success
        -- Unlock fly, speed, and aimbot features here
    else
        PaidKeyStatusLabel.Text = "Invalid Key! Try Again."
    end
end)
